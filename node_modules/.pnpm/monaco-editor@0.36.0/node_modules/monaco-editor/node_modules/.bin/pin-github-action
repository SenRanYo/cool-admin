#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/macos/WebProjects/cool-admin-vue/node_modules/.pnpm/pin-github-action@1.8.0/node_modules/pin-github-action/node_modules:/Users/macos/WebProjects/cool-admin-vue/node_modules/.pnpm/pin-github-action@1.8.0/node_modules:/Users/macos/WebProjects/cool-admin-vue/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/macos/WebProjects/cool-admin-vue/node_modules/.pnpm/pin-github-action@1.8.0/node_modules/pin-github-action/node_modules:/Users/macos/WebProjects/cool-admin-vue/node_modules/.pnpm/pin-github-action@1.8.0/node_modules:/Users/macos/WebProjects/cool-admin-vue/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../pin-github-action@1.8.0/node_modules/pin-github-action/bin.js" "$@"
else
  exec node  "$basedir/../../../../../pin-github-action@1.8.0/node_modules/pin-github-action/bin.js" "$@"
fi
