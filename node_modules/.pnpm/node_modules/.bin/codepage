#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/macos/WebProjects/cool-admin-vue/node_modules/.pnpm/codepage@1.14.0/node_modules/codepage/bin/node_modules:/Users/macos/WebProjects/cool-admin-vue/node_modules/.pnpm/codepage@1.14.0/node_modules/codepage/node_modules:/Users/macos/WebProjects/cool-admin-vue/node_modules/.pnpm/codepage@1.14.0/node_modules:/Users/macos/WebProjects/cool-admin-vue/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/macos/WebProjects/cool-admin-vue/node_modules/.pnpm/codepage@1.14.0/node_modules/codepage/bin/node_modules:/Users/macos/WebProjects/cool-admin-vue/node_modules/.pnpm/codepage@1.14.0/node_modules/codepage/node_modules:/Users/macos/WebProjects/cool-admin-vue/node_modules/.pnpm/codepage@1.14.0/node_modules:/Users/macos/WebProjects/cool-admin-vue/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../codepage/bin/codepage.njs" "$@"
else
  exec node  "$basedir/../codepage/bin/codepage.njs" "$@"
fi
